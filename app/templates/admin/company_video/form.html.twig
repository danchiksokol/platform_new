{% extends 'admin/base.html.twig' %}
{% block body %}
    {{ form_start(companyVideoForm) }}
    <div class="container">
        <div class="row">
            <table class="table table-hover table-bordered">
                <tbody>
                <tr>
                    <td>Компания</td>
                    <td>{{ form_row(companyVideoForm.company) }}</td>
                </tr>
                    <td>Видео</td>
                    <td>{{ form_row(companyVideoForm.video) }}</td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="row">
            <div class="col-md-6">{{ form_row(companyVideoForm.save) }}</div>
        </div>
    </div>
    {{ form_end(companyVideoForm) }}
{% endblock %}
