{% extends 'admin/base.html.twig' %}
{% block body %}
    {{ form_start(companyMaterialForm) }}
    <div class="container">
        <div class="row">
            <table class="table table-hover table-bordered">
                <tbody>
                <tr>
                    <td>Компания</td>
                    <td>{{ form_row(companyMaterialForm.company) }}</td>
                </tr>
                <tr>
                    <td>PDF</td>
                    <td>{{ form_row(companyMaterialForm.file) }}</td>
                </tr>
                    <td>Картинка</td>
                    <td>{{ form_row(companyMaterialForm.thumbnail) }}</td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="row">
            <div class="col-md-6">{{ form_row(companyMaterialForm.save) }}</div>
        </div>
    </div>
    {{ form_end(companyMaterialForm) }}
{% endblock %}
